ie-lab_router/src/components/Allmovies.jsx",["101","102","103"],"import React, { useEffect, useState } from 'react'\nimport { Col, Container, Form, Row } from 'react-bootstrap'\nimport OneCardMovie from './OneCardMovie'\nimport Axios from 'axios'\n// https://i.ibb.co/wdsJF41/32770.jpg\nexport default function Allmovies(props) {\n    const [movies , setMovie11] = useState([])\n    const [types , setTypes] = useState([])\n    const [selectMovies , setSelectMovie] = useState([])\n\n\n    useEffect(() => {\n        Axios.get('https://sei12.herokuapp.com/movei/json')\n        .then(res =>{     \n            setMovie11(res.data)\n            setSelectMovie(res.data) // select movies will take the first time all the movies ! \n            let typees = res.data.map(ele => ele.typee ) // add types base on the movies\n            typees.unshift('All') // add the All select top of the selects \n            setTypes(Array.from(new Set(typees))) // to make the array uniqe and non duplicate the elements\n            props.sendMovies(res.data);\n        })\n\n    }, [])\n//  show the selected movie after the filter\n   const  allmovies = selectMovies.map(movie =>{\n     return <OneCardMovie movie= {movie} />\n   }) \n// to add the selects  type\nlet allSelect = types.map(ele => <option value={ele}>{ele}</option>)\n// function to filtet the movie by the type \nconst onChangeHandler = (e) =>{\n    let value = e.target.value\n    if (value == \"All\") { // if the select all show all the movies \n        setSelectMovie(movies)\n    }else { // if not show only the movie type amd we use filter method ! \n        setSelectMovie(movies.filter(movie => movie.typee == value))\n    }\n}\n    return (\n        <div>\n            <Container >\n                <h1 className=\"mt-5\" style={{ textAlign: \"center\" }}>All movies</h1>\n                <Row className=\"justify-content-md-center\" >\n                    <Col md=\"4\" >\n                        <Form>\n                            <Form.Group controlId=\"exampleForm.SelectCustom\">\n                                <Form.Label>Type of the movie</Form.Label>\n                                <Form.Control as=\"select\" custom onChange ={onChangeHandler}>\n                                {allSelect}\n                                </Form.Control>\n                            </Form.Group>\n                        </Form>\n                    </Col>\n                </Row>\n                {/* movies cards */}\n                <Row className=\"justify-content-md-center\">\n                    {allmovies}\n                </Row>\n            </Container>\n        </div>\n    )\n}","/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/react-movie-lab_router/src/components/Footer.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/react-movie-lab_router/src/components/OneCardMovie.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/react-movie-lab_router/src/components/About.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/index.js",[],["104","105"],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/reportWebVitals.js",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/App.js",["106","107"],"import { useEffect, useState } from \"react\";\nimport { BrowserRouter as Router, Route, useHistory } from \"react-router-dom\";\nimport jwt_decode from \"jwt-decode\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport \"./App.css\";\nimport Footer from \"./components/Footer\";\nimport NavBar from \"./components/NaveBar\";\nimport AllMovies from \"./components/Allmovies\";\nimport Home from \"./components/Home\";\nimport About from \"./components/About\";\nimport ShowOneMovie from \"./components/ShowOneMovie\";\nimport Login from \"./components/Login\";\nimport Signup from \"./components/Signup\";\n\nfunction App() {\n\n  const [selectMovie, setSelectMovie] = useState({});\n  const history = useHistory();\n  const [auth, setAuth] = useState({ currentUser: null, isLoggedIn: false });\n\n  const userLogin = () => {\n    if (localStorage.jwtToken) {\n      const jwtToken = localStorage.jwtToken;\n      const currentUser = jwt_decode(jwtToken, \"SECRET\").user;\n      setAuth({ currentUser, isLoggedIn: true });\n    } else {\n      setAuth({ currentUser: null, isLoggedIn: false });\n    }\n    console.log(\"The current User is: \", auth.currentUser);\n  };\n\n  useEffect(userLogin, []);\n\n  return (\n    <div className=\"\">\n      <Router>\n        <NavBar />\n        <Route path=\"/movies/:id\">\n          <ShowOneMovie selectMovie={selectMovie} />\n        </Route>\n        <Route exact path=\"/movies\">\n          <AllMovies setSelectMovie={setSelectMovie} />\n        </Route>\n        <Route path=\"/about\">\n          <About />\n        </Route>\n        <Route path=\"/login\">\n          <Login loginCallback={userLogin} />\n        </Route>\n        <Route path=\"/signup\">\n          <Signup loginCallback={userLogin} />\n        </Route>\n        <Route exact path=\"/\">\n          <Home />\n        </Route>\n        <Footer />\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/NaveBar.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/ShowOneMovie.jsx",["108"],"import axios from 'axios'\nimport React, { useState,useEffect } from 'react'\nimport { Col, Container, Row } from 'react-bootstrap'\nimport {useParams} from 'react-router-dom';\nexport default function ShowOneMovie(props) {\n    const {id} = useParams()\n    const [selectMovie , setSelectMovie]= useState(props.selectMovie)\n    const {name , link ,history , typee , YourName  } = selectMovie\n    \n    useEffect(() => {\n            if (!name) {\n                axios.get('https://sei12.herokuapp.com/movei/json')\n                .then(res =>{     \n                    let movie = res.data.find(ele => ele._id === id)\n                    setSelectMovie(movie)\n                })\n\n            }\n    \n    }, [])\n\n\n    return (\n        <>\n            <Container className=\"mt-5\" >\n                <Row >\n                    <Col md=\"6\" >\n                        <img width=\"100%\" src={link} alt=\"\" srcset=\"\" />\n                    </Col>\n                    <Col md=\"6\">\n                        <h2>{name}</h2>\n                        <h2> {history}</h2>\n                        <h2>{YourName}</h2>\n                        <h2>{typee}</h2>\n                    </Col>\n                </Row>\n\n            </Container>\n        </>\n    )\n}\n","/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/Allmovies.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/About.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/Home.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/Footer.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/OneCardMovie.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/Login.jsx",[],"/Users/ibrahimshaheen/Desktop/SEI-ALAHSA/Week-8/MEART-AUTH/React-Movie/src/components/Signup.jsx",["109"],"import React, { useState, useEffect } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport { Row, Form, Col, Button, Alert } from \"react-bootstrap\";\nimport axios from \"axios\";\n\nexport default function Singup(props) {\n  const history = useHistory();\n\n  const [user, setUser] = useState({}); // user info\n  const [register, setRegister] = useState(true); // to show aleart\n\n  //to add the input inside user\n  const onChangeInput = ({ target: { name, value } }) => {\n    setUser({ ...user, [name]: value });\n  };\n  // to add the user info to database\n  const onSubmit = (event) => {\n    event.preventDefault();\n    axios\n      .post(\"http://localhost:5000/api/user/register\", user)\n      .then((res) => {\n        const user = res.data.user;\n        if (user) {\n          history.push(\"/login\");\n        } else {\n          setTimeout(() => {\n            setRegister(false);\n          }, 1000);\n        }\n      })\n      .catch((err) => console.log(err));\n  };\n\n  return (\n    <>\n      {!register && (\n        <Alert variant={\"danger\"}>\n          The email is already in use. Please change the email\n        </Alert>\n      )}\n      <Form className=\"mt-5\">\n        <Row className=\"justify-content-center mt-5\">\n          <Col md={8}>\n            <Form.Row>\n              <Col md={6}>\n                <Form.Label>First name</Form.Label>\n                <Form.Control\n                  placeholder=\"First name\"\n                  name=\"name\"\n                  onChange={(e) => onChangeInput(e)}\n                />\n              </Col>\n              <Col md={6}>\n                <Form.Label>Last name</Form.Label>\n                <Form.Control\n                  placeholder=\"Last name\"\n                  name=\"lastName\"\n                  onChange={(e) => onChangeInput(e)}\n                />\n              </Col>\n              <Col md={12}>\n                <Form.Label>image</Form.Label>\n                <Form.Control\n                  placeholder=\"image\"\n                  name=\"image\"\n                  onChange={(e) => onChangeInput(e)}\n                />\n              </Col>\n            </Form.Row>\n            <Form.Row>\n              <Form.Group as={Col} controlId=\"formGridEmail\">\n                <Form.Label>Email</Form.Label>\n                <Form.Control\n                  type=\"email\"\n                  placeholder=\"Enter email\"\n                  name=\"email\"\n                  onChange={(e) => onChangeInput(e)}\n                />\n              </Form.Group>\n\n              <Form.Group as={Col} controlId=\"formGridPassword\">\n                <Form.Label>Password</Form.Label>\n                <Form.Control\n                  type=\"password\"\n                  placeholder=\"Password\"\n                  name=\"password\"\n                  onChange={(e) => onChangeInput(e)}\n                />\n              </Form.Group>\n            </Form.Row>\n            <Button\n              variant=\"primary\"\n              type=\"submit\"\n              onClick={(e) => onSubmit(e)}\n            >\n              Submit\n            </Button>\n          </Col>\n        </Row>\n      </Form>\n    </>\n  );\n}",{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":1,"column":17,"nodeType":"116","messageId":"117","endLine":1,"endColumn":26},{"ruleId":"114","severity":1,"message":"118","line":7,"column":35,"nodeType":"116","messageId":"117","endLine":7,"endColumn":41},{"ruleId":"114","severity":1,"message":"119","line":7,"column":50,"nodeType":"116","messageId":"117","endLine":7,"endColumn":54},{"ruleId":"114","severity":1,"message":"120","line":7,"column":56,"nodeType":"116","messageId":"117","endLine":7,"endColumn":65},{"ruleId":"114","severity":1,"message":"121","line":9,"column":8,"nodeType":"116","messageId":"117","endLine":9,"endColumn":20},{"ruleId":"114","severity":1,"message":"122","line":11,"column":8,"nodeType":"116","messageId":"117","endLine":11,"endColumn":13},{"ruleId":"114","severity":1,"message":"123","line":3,"column":27,"nodeType":"116","messageId":"117","endLine":3,"endColumn":33},{"ruleId":"114","severity":1,"message":"118","line":3,"column":35,"nodeType":"116","messageId":"117","endLine":3,"endColumn":41},{"ruleId":"114","severity":1,"message":"124","line":3,"column":43,"nodeType":"116","messageId":"117","endLine":3,"endColumn":48},{"ruleId":"125","severity":1,"message":"126","line":23,"column":8,"nodeType":"127","endLine