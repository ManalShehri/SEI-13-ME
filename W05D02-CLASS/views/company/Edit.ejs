    <h1>Edit <%=company.name%></h1>
    <div class="form">
      <form action="/company/update/<%=company._id%>?_method=put" method="POST">
          <div class="form-group">
              <label for="name">Company Name</label>
              <input type="text" class="form-control" name="name" value="<%=company.name%>">
          </div>
          <div class="form-group">
              <label for="name">Company Logo</label>
              <input type="text" class="form-control" name="logo" value="<%=company.logo%>">
          </div>
          <div class="form-group">
              <label for="name">Company City</label>
              <input type="text" class="form-control" name="city" value="<%=company.address%>">
          </div>
          <div class="form-group">
              <label for="name">Company Address</label>
              <input type="text" class="form-control" name="address" value="<%=company.city%>">
          </div>
          <div class="form-group">
              <label for="name">Company Telephone</label>
              <input type="text" class="form-control" name="telephone" value="<%=company.telephone%>">
          </div>
          <% cars.forEach(car => { %>
            <div class="custom-control custom-checkbox">
                <input type="checkbox" class="custom-control-input" name="cars"  <% company.cars.includes(car._id) ? "checked" : "" %>  value="<%=car.id%>">
                <label class="custom-control-label" for="customCheck1" name="cars"><%=car.name%> </label>
            </div>
          <% }) %>
          <input type="submit" value="Edit company" class="btn btn-primary"/>
      </form>
  </div>